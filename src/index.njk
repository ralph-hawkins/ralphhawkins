---
layout: base
---
{% set latestPost = collections.weeknotes | reverse | first %}
{% if latestPost %}
<script defer>
  window.location.href = "{{ latestPost.url }}";
</script>
<noscript>
  <meta http-equiv="refresh" content="0; url={{ latestPost.url }}">
</noscript>
<p>Redirecting to latest weeknotes...</p>
<p><a href="{{ latestPost.url }}">Click here if not redirected</a></p>
{% else %}
<p>No posts found</p>
{% endif %}
